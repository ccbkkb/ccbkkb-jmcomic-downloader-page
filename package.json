{
  "name": "jmcomic-downloader-web",
  "private": true,
  "scripts": {
    "page:build": "cd page && pnpm run build",
    "page:dev": "cd page && pnpm run dev",
    "page:lint": "cd page && pnpm run lint",
    "page:preview": "cd page && pnpm run preview",
    "worker:dev": "cd worker && pnpm run dev",
    "worker:deploy": "cd worker && pnpm run deploy",
    "worker:start": "cd worker && pnpm run start",
    "worker:cf-typegen": "cd worker && pnpm run cf-typegen",
    "worker:test": "cd worker && pnpm run test",
    "dev": "concurrently \"pnpm run worker:dev\" \"wait-on tcp:localhost:8787 && pnpm run page:dev\""
  },
  "packageManager": "pnpm@10.28.0",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "wait-on": "^9.0.3"
  }
}